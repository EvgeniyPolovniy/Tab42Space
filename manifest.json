{
  "manifest_version": 2,

  "name": "Tab42Space",
  "description": "This extension for everything!",
  "version": "0.1",

  "icons": {
    "16": "./img/icon/16x16.png",
    "32": "./img/icon/32x32.png",
    "48": "./img/icon/48x48.png",
    "64": "./img/icon/48x48.png",
    "128": "./img/icon/128x128.png",
    "256": "./img/icon/256x256.png"
  },

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "permissions": [
    "https://*/*",
    "http://*/*",
    "storage",
    "tabs"
  ],

  "background" : {
    "page": "background.html",
    "persistent": false
  }
}
